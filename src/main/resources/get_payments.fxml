                  <Font name="System Bold" size="18.0" />
               </font></Label>
            <TextField fx:id="startDayTF" layoutX="138.0" layoutY="77.0" />
            <TextField fx:id="startMonthTF" layoutX="138.0" layoutY="133.0" />
            <Label layoutX="56.0" layoutY="81.0" text="Starting day" />
            <Label layoutX="53.0" layoutY="137.0" text="Starting month" />
            <Label layoutX="329.0" layoutY="81.0" text="Ending day" />
            <Label layoutX="326.0" layoutY="137.0" text="Ending month" />
            <TextField fx:id="endDayTF" layoutX="404.0" layoutY="77.0" />
