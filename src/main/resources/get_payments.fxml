<GridPane styleClass="transaction" stylesheets="@backgrounds/StyleSheets.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.loose.fis.registration.example.controllers.GetSalariesController">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="400.0" prefWidth="600.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
         <children>
            <Button layoutX="413.0" layoutY="188.0" mnemonicParsing="false" onAction="#handleGetSalaries" prefHeight="25.0" prefWidth="131.0" text="Get salaries" />
            <Label fx:id="salariesLabel" layoutX="251.0" layoutY="219.0" prefHeight="86.0" prefWidth="241.0" text="Stub text for total salaries">
